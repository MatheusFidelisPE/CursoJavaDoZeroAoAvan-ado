package oo.hierarquia.desafio;

public class Civic extends Carro  implements Esportivo{
	
	public Civic(){
		super(260);
		delta = 15;
	}
	
	public boolean acelerar() {
		super.acelerar();
		super.acelerar();
		super.acelerar();
		return true;
	}
	@Override
	public void ligarTurbo() {
		delta = 35;
	}
	@Override
	public void desligarTurbo() {
		delta = 15;
	}
	
}